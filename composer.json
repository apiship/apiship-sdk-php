{
  "name": "apiship/apiship-sdk-php",
  "description": "Inegration (SDK) with Apiship API",
  "keywords": [
    "Apiship",
    "sdk",
    "integration",
    "REST"
  ],
  "minimum-stability": "dev",
  "type": "extension",
  "authors": [
    {
      "name": "ApiShip",
      "email": "info@apiship.ru"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apiship\\": "src"
    }
  },
  "require": {
    "php": ">=5.5.0"
  },
  "suggest": {
     "guzzlehttp/guzzle": "Needed if you want to use guzzle adapter",
  }
}
